#include <iostream>

using namespace std;

int main()
{
    int a, b, c;
    
    cout << "Number 1: ";
    cin >> a;
    
    cout << "Number 2: ";
    cin >> b;
    
    cout << "Number 3: ";
    cin >> c;
    
    if((a>b) && (b>c)){
        cout << a << "is max number" << endl;
    }
    else if ((b>a) && (b>c)) {
        cout << b << "is max number" << endl; 
    }
    else if ((c>a) && (c>b)) {
        cout << c << "is max number" << endl; 
    }
    else {
        cout << "invalid input";
    }
    
    
    return 0;
}
